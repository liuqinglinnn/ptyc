page {
  background-color: #f5f5f5;
}
.zhezhao {
  width: 100%;
  height: 1700rpx;
  position: absolute;
  z-index: 0;
  top: 0;
  background: -webkit-linear-gradient(#0cca80, #f5f5f5);
}
.body {
  position: absolute;
  width: 100%;
  height: auto;
}
.title1 {
  font-weight: bolder;
  font-size: 45rpx;
  margin-top: 20rpx;
  letter-spacing: 5rpx;
  width: 95%;
  margin-left: 2.75%;
  z-index: 1;
}
.title2 {
  font-weight: bolder;
  font-size: 45rpx;
  margin-top: 5rpx;
  letter-spacing: 5rpx;
  width: 95%;
  margin-left: 2.75%;
  margin-bottom: 25rpx;
}
.stockhead {
  width: 95%;
  margin-left: 2.5%;
  border-radius: 25rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.stockhead .stockhead_ {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding-left: 25rpx;
  padding-right: 15rpx;
  border-right: 2rpx solid #ebebeb;
  border-left: 2rpx solid #ebebeb;
}
.stockhead .stockhead_ .name {
  font-weight: normal;
}
.stockhead .stockhead_ .num {
  font-size: 25rpx;
  opacity: 0.5;
}
.container {
  margin-top: 30rpx;
  padding: 0;
  border-radius: 25rpx;
  overflow: hidden;
  margin-left: 2.5%;
  width: 95%;
  background-color: white;
}
.container .canvas {
  border-radius: 5rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 200px;
}
.operate_text {
  margin-left: 45rpx;
  margin-top: 25rpx;
  font-weight: bolder;
  font-size: 35rpx;
  letter-spacing: 5rpx;
  width: 80rpx;
  padding-bottom: 15rpx;
  border-bottom: 2px solid #0cca80;
}
.operate_text2 {
  margin-left: 45rpx;
  margin-top: 25rpx;
  font-weight: bolder;
  font-size: 35rpx;
  letter-spacing: 5rpx;
  width: 80rpx;
  padding-bottom: 15rpx;
  border-bottom: 2px solid #ffd524;
}
.operate {
  border-top: 1rpx solid #f5f5f5;
  margin-left: 2.5%;
  width: 95%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 400rpx;
}
.operate .operate_content {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.operate .operate_content .oleft {
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.operate .operate_content .oleft .ruku {
  width: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  height: 150rpx;
  margin: 20rpx;
  border-radius: 35rpx;
  color: #0ac785;
  letter-spacing: 5rpx;
  background-color: #ffffff;
  box-shadow: 0px 0px 10px #afafaf;
}
.operate .operate_content .oleft .ruku .rukupic {
  width: 75rpx;
  height: 75rpx;
  margin-right: 10rpx;
  margin-left: 40rpx;
}
.operate .operate_content .oleft .ruku .rukupic .rukupic_ {
  width: 75rpx;
  height: 75rpx;
}
.operate .operate_content .oleft .ruku .rukutex {
  font-weight: bolder;
  margin-left: 10rpx;
  font-size: 30rpx;
}
.operate .operate_content .oleft2 {
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.operate .operate_content .oleft2 .ruku2 {
  width: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  height: 340rpx;
  margin: 20rpx;
  border-radius: 35rpx;
  color: #0ac785;
  letter-spacing: 5rpx;
  background-color: #ffffff;
  box-shadow: 0px 0px 10px #afafaf;
}
.operate .operate_content .oleft2 .ruku2 .rukupic2 {
  width: 75rpx;
  height: 75rpx;
  margin-right: 10rpx;
  margin-left: 40rpx;
}
.operate .operate_content .oleft2 .ruku2 .rukupic2 .rukupic_2 {
  width: 75rpx;
  height: 75rpx;
}
.operate .operate_content .oleft2 .ruku2 .rukutex2 {
  font-weight: bolder;
  margin-left: 10rpx;
  font-size: 30rpx;
}
.other {
  margin-bottom: 30rpx;
  margin-left: 5%;
  border-bottom: 2px solid #ebebeb;
  border-top: 2px solid #ebebeb;
  width: 90%;
  display: flex;
  flex-direction: column;
}
.other .other_content {
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  margin-left: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  font-size: 30rpx;
}
.bottomtext {
  font-size: 30rpx;
  opacity: 0.7;
  text-align: center;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.showallorder {
  background-color: white;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
  margin-top: 25rpx;
  padding-left: 25rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.showallorder .showallorder_text {
  font-size: 25rpx;
}
.showallorder .showallorder_ {
  position: absolute;
  right: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  width: 100rpx;
}
.showallorder .showallorder_ .showallorder_text {
  font-size: 30rpx;
  width: 60rpx;
}
.showallorder .showallorder_ .showallorder_image {
  height: 35rpx;
  width: 35rpx;
}
