page {
  background-color: #f5f5f5;
}
.search {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
}
.search .search_ {
  background-color: #f5f5f5;
  height: 75rpx;
  width: 75%;
  margin-left: 50rpx;
  border-radius: 75rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.search .search_ .search_icon {
  width: 50rpx;
  height: 50rpx;
  margin-left: 25rpx;
}
.search .search_ .search_input {
  font-size: 30rpx;
  margin-left: 15rpx;
}
.search .cancel {
  font-size: 30rpx;
  margin-left: 30rpx;
  opacity: 0.7;
}
.admin {
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
}
.admin .adminpeople {
  width: 100%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  margin-bottom: 25rpx;
  background-color: white;
  align-items: center;
}
.admin .adminpeople .people_for_left .people_for_left_img {
  width: 100rpx;
  height: 100rpx;
}
.admin .adminpeople .people_for_right {
  margin-left: 25rpx;
}
.admin .people {
  background-color: white;
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
}
.admin .people .people_for {
  width: 100%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  background-color: white;
  align-items: center;
}
.admin .people .people_for .people_for_left .people_for_left_img {
  width: 100rpx;
  height: 100rpx;
}
.admin .people .people_for .people_for_right {
  margin-left: 25rpx;
}
.admin .people .people_buttom {
  position: absolute;
  right: 50rpx;
}
